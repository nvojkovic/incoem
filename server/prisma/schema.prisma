generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Client {
  id                  Int       @id @default(autoincrement())
  createdAt           DateTime  @default(now())
  title               String
  userId              String
  data                Json      @default("{}")
  scenarios           Json      @default("[]")
